<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context="com.example.mitubhowmick10.version.AnimFirstAid.Main_Page">

        <Button android:id="@+id/fvr"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:textStyle="bold"
            android:background="@color/colorAccent"
            android:text="Fever"
            android:textSize="15dp"
            android:textColor="@color/colorPrimaryDark"
            tools:layout_editor_absoluteX="33dp"
            tools:layout_editor_absoluteY="50dp"
            android:layout_marginBottom="5dp"/>
        <Button android:id="@+id/d2"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:textStyle="bold"
            android:background="@color/colorAccent"
            android:layout_below="@+id/fvr"
            android:text="Allergy"
            android:textSize="15dp"
            android:textColor="@color/colorPrimaryDark"
            tools:layout_editor_absoluteX="33dp"
            tools:layout_editor_absoluteY="50dp"
            android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/d3"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_below="@+id/d2"
        android:text="Anemia"
        android:textSize="15dp"
        android:textColor="@color/colorPrimaryDark"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/d4"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_below="@+id/d3"
        android:text="Asthma"
        android:textSize="15dp"
        android:textColor="@color/colorPrimaryDark"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/d5"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/d4"
        android:text="Appendix"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/d6"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/d5"
        android:text="Bronchitis"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/d7"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_alignEnd="@+id/d5"
        android:layout_alignRight="@+id/d5"
        android:layout_below="@+id/d6"
        android:text="Diarrhea"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/d8"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Chkingunya"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/d9"
        android:layout_below="@+id/d8"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_centerHorizontal="true"
        android:text="Dengue"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/d10"
        android:layout_below="@+id/d9"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_centerHorizontal="true"
        android:text="Cholera"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/d11"
        android:layout_below="@+id/d10"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:layout_centerHorizontal="true"
        android:text="Typhoid"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginBottom="5dp"/>
    <Button android:id="@+id/back"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:textSize="20dp"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:text="Back"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="30dp"/>

</RelativeLayout>
